{
  "name": "unwrap",
  "version": "0.0.1",
  "description": "A utility to make it easier to work with unknown types in Typescript",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "cover": "jest --coverage"
  },
